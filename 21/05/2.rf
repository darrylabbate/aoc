f = open(arg[1])
while !eof(f) {
    if !(read(f) ~ /(\d+),(\d+) -> (\d+),(\d+)/)
        break
    dx = $1 == $3 ? 0 : $1 < $3 ? 1 : -1
    dy = $2 == $4 ? 0 : $2 < $4 ? 1 : -1
    grid[$1,$2]++
    while $1 != $3 || $2 != $4 {
        $1 += dx
        $2 += dy
        grid[$1,$2]++
    }
}
close(f)
for g in grid
    for c in g
        if c > 1
            n++
print n
